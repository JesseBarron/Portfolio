    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-icons": "^2.2.7",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-transition-group": "^2.3.1",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "slate": "^0.33.6",
    "slate-react": "^0.12.6",
    "slate-schema-violations": "^0.1.11",
    "socket.io-client": "^2.1.0",
    "steerjs": "^2.1.1"
  },
  "devDependencies": {
