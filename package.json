{
  "name": "boilerplate",
  "version": "1.0.0",
  "description": "BoilerPlateForMongoDB",
  "main": "index.js",
  "author": "Jesse Barron",
  "license": "MIT",
  "scripts": {
    "seed": "node ./server_build/scripts/seed.js",
    "build": "webpack",
    "build-watch": "webpack -w --mode development",
    "start": "node ./server_build/index.js",
    "start-dev": "NODE_ENV='development' yarn build-watch & tsc --pretty -w & nodemon ./server_build/index.js",
    "test": "NODE_ENV='test' DB_URL='mongodb://localhost:27017/portfolio-TEST' mocha --require ts-node/register --files ./server/**/*.test.ts ./server/**/**/*.test.ts",
    "test-client": "NODE_ENV='test' DB_URL='mongodb://localhost:27017/portfolio-TEST' yarn seed && yarn start & mocha --require babel-polyfill ./client/testSetup.js --compilers babel-core/register ./client/**/*.test.js ./client/**/**/*.test.js"
  },
  "dependencies": {
    "@feathersjs/authentication": "^2.1.5",
