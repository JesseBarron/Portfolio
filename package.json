    "@feathersjs/socketio": "^3.2.1",
    "@feathersjs/socketio-client": "^1.1.0",
    "argon2": "^0.19.0",
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.2.1",
    "jwt-decode": "^2.2.0",
    "mongodb": "^3.0.7",
    "mongoose": "^5.0.17",
